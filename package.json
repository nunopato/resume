{
  "name": "resume",
  "version": "0.0.1",
  "description": "a resume built with HTML/CSS for high quality printing and easy maintenance",
  "main": "webpack.config.js",
  "author": "Nuno Pinge",
  "license": "MIT",
  "homepage": "https://github.com/pinge/resume",
  "repository": {
    "type": "git",
    "url": "https://github.com/pinge/resume"
  },
  "bugs": {
    "url": "https://github.com/pinge/resume/issues"
  },
  "scripts": {
    "start": "better-npm-run start",
    "start:dash": "better-npm-run start:dash",
    "build": "better-npm-run build",
    "build:dll": "webpack --config dll.webpack.config.js",
    "test": "jest --verbose"
  },
  "betterScripts": {
    "start": {
      "command": "./node_modules/webpack-dev-server/bin/webpack-dev-server.js",
      "env": {
        "NODE_ENV": "development",
        "BABEL_ENV": "development"
      }
    },
    "build": {
      "command": "webpack",
      "env": {
        "NODE_ENV": "production",
        "BABEL_ENV": "production"
      }
    }
  },
  "jest": {
    "moduleDirectories": [
      "node_modules",
      "src"
    ]
  },
  "keywords": [
    "react",
    "webpack",
    "webpack 3",
    "babel",
    "es6",
    "es7",
    "styled-components",
    "template",
    "eslint",
    "tests"
  ],
  "devDependencies": {
    "babel-eslint": "^6.1.2",
    "babel-loader": "^7.1.4",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "0.0.8",
    "babel-preset-react": "^6.11.1",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-2": "^6.16.0",
    "better-npm-run": "0.0.13",
    "css-loader": "^0.28.4",
    "eslint": "^3.19.0",
    "eslint-config-standard": "^5.3.5",
    "eslint-plugin-promise": "^2.0.1",
    "eslint-plugin-react": "^5.2.2",
    "eslint-plugin-standard": "^2.0.0",
    "react": "^15.6.1",
    "style-loader": "^0.18.2",
    "styled-components": "^2.1.2",
    "whatwg-fetch": "^1.0.0"
  },
  "dependencies": {
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "webpack": "^3.12.0",
    "webpack-dashboard": "^1.0.0-5",
    "webpack-dev-server": "^2.9.1"
  }
}
